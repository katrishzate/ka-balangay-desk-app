        <TextBox HorizontalAlignment="Left" Margin="430,187,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <TextBlock HorizontalAlignment="Left" Margin="349,216,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Century Gothic" FontSize="10"><Run Language="en-ph" Text="Personnel ID"/></TextBlock>
        <TextBox HorizontalAlignment="Left" Margin="430,213,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="185"/>
        <TextBlock HorizontalAlignment="Left" Margin="349,242,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontWeight="Bold" FontFamily="Century Gothic" FontSize="10"><Run Language="en-ph" Text="Date"/></TextBlock>
